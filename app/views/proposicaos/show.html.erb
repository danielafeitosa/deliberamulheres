<%- model_class = Proposicao -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:reuniao_id) %>:</strong></dt>
  <dd><%= @proposicao.reuniao_id %></dd>
  <dt><strong><%= model_class.human_attribute_name(:sigla) %>:</strong></dt>
  <dd><%= @proposicao.sigla %></dd>
  <dt><strong><%= model_class.human_attribute_name(:id_proposicao) %>:</strong></dt>
  <dd><%= @proposicao.id_proposicao %></dd>
  <dt><strong><%= model_class.human_attribute_name(:ementa) %>:</strong></dt>
  <dd><%= @proposicao.ementa %></dd>
  <dt><strong><%= model_class.human_attribute_name(:resultado) %>:</strong></dt>
  <dd><%= @proposicao.resultado %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              proposicaos_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_proposicao_path(@proposicao), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              proposicao_path(@proposicao),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
