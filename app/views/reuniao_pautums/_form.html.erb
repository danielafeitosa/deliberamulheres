<%= form_for @reuniao_pautum, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :orgao_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :orgao_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :cod_reuniao, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :cod_reuniao, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :data, :class => 'control-label' %>
    <div class="controls">
      <%= f.date_select :data, :class => 'date_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :horario, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :horario, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :local, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :local, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :estado, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :estado, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :tipo, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :tipo, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :titulo_reuniao, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :titulo_reuniao, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :objeto, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_area :objeto, :class => 'text_area' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                reuniao_pautums_path, :class => 'btn' %>
  </div>
<% end %>
