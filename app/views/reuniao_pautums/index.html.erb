<%- model_class = ReuniaoPautum -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:orgao_id) %></th>
      <th><%= model_class.human_attribute_name(:cod_reuniao) %></th>
      <th><%= model_class.human_attribute_name(:data) %></th>
      <th><%= model_class.human_attribute_name(:horario) %></th>
      <th><%= model_class.human_attribute_name(:local) %></th>
      <th><%= model_class.human_attribute_name(:estado) %></th>
      <th><%= model_class.human_attribute_name(:tipo) %></th>
      <th><%= model_class.human_attribute_name(:titulo_reuniao) %></th>
      <th><%= model_class.human_attribute_name(:objeto) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @reuniao_pauta.each do |reuniao_pautum| %>
      <tr>
        <td><%= link_to reuniao_pautum.id, reuniao_pautum_path(reuniao_pautum) %></td>
        <td><%= reuniao_pautum.orgao_id %></td>
        <td><%= reuniao_pautum.cod_reuniao %></td>
        <td><%= reuniao_pautum.data %></td>
        <td><%= reuniao_pautum.horario %></td>
        <td><%= reuniao_pautum.local %></td>
        <td><%= reuniao_pautum.estado %></td>
        <td><%= reuniao_pautum.tipo %></td>
        <td><%= reuniao_pautum.titulo_reuniao %></td>
        <td><%= reuniao_pautum.objeto %></td>
        <td><%=l reuniao_pautum.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_reuniao_pautum_path(reuniao_pautum), :class => 'btn btn-mini' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      reuniao_pautum_path(reuniao_pautum),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_reuniao_pautum_path,
            :class => 'btn btn-primary' %>
