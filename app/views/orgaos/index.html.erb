<h1>Listing orgaos</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Sigla</th>
    <th>Id tipo orgao</th>
    <th>Id orgao legislativo</th>
    <th>Descricao</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @orgaos.each do |orgao| %>
  <tr>
    <td><%= orgao.name %></td>
    <td><%= orgao.sigla %></td>
    <td><%= orgao.id_tipo_orgao %></td>
    <td><%= orgao.id_orgao_legislativo %></td>
    <td><%= orgao.descricao %></td>
    <td><%= link_to 'Show', orgao %></td>
    <td><%= link_to 'Edit', edit_orgao_path(orgao) %></td>
    <td><%= link_to 'Destroy', orgao, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Orgao', new_orgao_path %>
